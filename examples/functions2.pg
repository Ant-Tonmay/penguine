{   

    func prime(number){
        for (i=2;i<number;i = i + 1)
        {
            if (number%i==0)
            {
                return false;
            }
        }
        return true;
    }

    func main()
    {
        if (prime(10))
        {
            print("Prime");
        }
        else
        {
            print("Not Prime");
        }
    }
}